<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal" data-whatever="@mdo">Add your dog here!</button>

<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
  aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Add your dog!</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form>
          <div class="form-group">
            <label for="dogName">Name</label>
            <input type="text" class="form-control" id="input-dog-name" placeholder="Buddy">
          </div>
          <div class="form-group">
            <label for="dogName">Breed</label>
            <input type="text" class="form-control" id="input-dog-breed" placeholder="Labrador">
          </div>
          <div class="form-group">
            <label for="exampleFormControlSelect1">Size</label>
            <select class="form-control" id="input-dog-size">
              <option value="1">Small (1-34 lbs)</option>
              <option value="2">Medium (35-65 lbs)</option>
              <option value="3">Large (66+ lbs)</option>
            </select>
          </div>
          <div class="form-group">
            <label for="dogName">Picture</label>
            <input type="text" class="form-control" id="input-dog-pic" placeholder="link to picture">
          </div>
          <div class="form-group">
            <label for="exampleFormControlSelect1">Personality</label>
            <select class="form-control" id="input-dog-personality">
              <option value="1">Calm</option>
              <option value="2">Energetic</option>
              <option value="3">Insane</option>
              <option value="4">Weird</option>
            </select>
          </div>
          <div class="form-group">
            <label for="exampleFormControlSelect1">Age</label>
            <select class="form-control" id="input-dog-age">
              <option value="1">0-3</option>
              <option value="2">4-9</option>
              <option value="3">>=10</option>
            </select>
          </div>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" data-dismiss="modal" id="add-Dog">Add Dog!</button>
      </div>
    </div>
  </div>
</div>
<script>
    function signOut() {
    var auth2 = gapi.auth2.getAuthInstance();
    auth2.signOut().then(function () {
      console.log('User signed out.');
    });
    loggedIn = false;
  }
</script>